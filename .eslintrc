{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "semi": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "arrow-parens": "off"
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  }
}
